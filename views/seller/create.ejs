<form action="/seller/create" method="post">

    <table border="1" cellpadding="10" style="border-collapse: collapse;">
        <tr>
            <td>Nama Seller</td>
            <td><input type="text" name="nama_seller"/></td>
        </tr>

        <tr>
            <td>Nama Toko</td>
            <td><input type="text" name="nama_toko"/></td>
        </tr>
        
        <tr>
            <td>No Telepon</td>
            <td><input type="text" name="no_telp"/></td>
        </tr>

        <tr>
            <td>Email</td>
            <td><input type="email" name="email"/></td>
        </tr>

        <tr>
            <td>Alamat :</td>
            <td><textarea name="alamat"cols="30" rows="10"></textarea></td>
        </tr>

    </table>

    <br>
    <input type="submit" value="Simpan">

</form>